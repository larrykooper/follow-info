    <tr>
      <th><u>Follows Me Nbr</u></th>
      <th><u>Name</u></th>     
      <th><u>I Follow?</u></th>
    </tr>
  </thead>
  
  <%= render(:partial => "foller_row", :collection => @followings) %> 
</table>